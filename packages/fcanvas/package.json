{
  "name": "fcanvas",
  "version": "0.0.1",
  "description": "a monorepo for fcanvas",
  "scripts": {
    "build": "tsup",
    "dev": "tsx ./src/index.ts",
    "prepublishOnly": "pnpm test && pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.mjs",
  "jsdelivr": "dist/index.global.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.7",
    "lodash.clonedeep": "^4.5.0",
    "mitt": "^3.0.0",
    "path-cross": "^0.0.3",
    "plist": "^3.0.5",
    "tsup": "^6.0.1",
    "vitest": "^0.13.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Tachibana-Shin/fcanvas-next.git"
  },
  "homepage": "https://fcanvas.js.org",
  "keywords": [
    "fcanvas-next",
    "Tachibana Shin",
    "Tachibana-Shin",
    "fcanvas-next",
    "fcanvas",
    "canvas",
    "2d",
    "html 5"
  ],
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format"
    }
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4"
  }
}
