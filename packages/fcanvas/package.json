{
  "name": "fcanvas",
  "version": "1.0.10",
  "description": "This is a simple canvas library for drawing on the fly.",
  "scripts": {
    "build": "pnpm auto-export && tsup",
    "auto-export": "tsx scripts/auto-export.ts",
    "dev": "vite dev",
    "prepublishOnly": "pnpm test && pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.browser.mjs",
  "jsdelivr": "dist/index.global.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/plist": "^3.0.2",
    "canvas": "^2.9.3",
    "dommatrix": "^1.0.3",
    "fetch-polyfill": "^0.8.2",
    "happy-dom": "^6.0.4",
    "jsdom": "^20.0.0",
    "plist": "^3.0.6",
    "tsup": "^6.2.1",
    "tsx": "^3.8.0",
    "vite": "^3.0.4",
    "vitest": "^0.21.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/tachibana-shin/fcanvas.git"
  },
  "homepage": "https://fcanvas.js.org",
  "keywords": [
    "fcanvas",
    "konva",
    "reactivity",
    "2d",
    "canvas",
    "Tachibana Shin",
    "tachibana-shin"
  ],
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format"
    }
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@vue-reactivity/watch": "^0.2.0",
    "@vue/reactivity": "^3.2.37",
    "@vue/shared": "^3.2.37",
    "gsap": "^3.10.4",
    "path-normalize": "^6.0.6",
    "tmx-tiledmap": "^1.2.1"
  },
  "peerDependencies": {
    "@vue-reactivity/watch": "^0.2.0",
    "@vue/reactivity": "^3.2.37",
    "@vue/shared": "^3.2.37",
    "gsap": "^3.10.4",
    "path-normalize": "^6.0.6"
  }
}
