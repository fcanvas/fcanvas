{
  "name": "fcanvas",
  "version": "1.0.0",
  "description": "a monorepo for packages",
  "scripts": {
    "build": "pnpm auto-export && tsup",
    "auto-export": "tsx scripts/auto-export.ts",
    "dev": "vite dev",
    "prepublishOnly": "pnpm test && pnpm build",
    "test": "vitest --config ./vitest.config.ts run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.mjs",
  "jsdelivr": "dist/index.global.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/plist": "^3.0.2",
    "canvas": "2.9.2",
    "jsdom": "^20.0.0",
    "plist": "^3.0.5",
    "tsup": "^6.1.3",
    "tsx": "^3.7.1",
    "vite": "^2.9.13",
    "vitest": "^0.17.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/tachibana-shin/fcanvas.git"
  },
  "homepage": "https://fcanvas.js.org",
  "keywords": [
    "fcanvas",
    "konva",
    "reactivity",
    "2d",
    "canvas",
    "Tachibana Shin",
    "tachibana-shin"
  ],
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format"
    }
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@vue-reactivity/watch": "^0.2.0",
    "@vue/reactivity": "^3.2.37",
    "@vue/shared": "^3.2.37",
    "gsap": "^3.10.4",
    "path-normalize": "^6.0.6",
    "tmx-tiledmap": "^1.2.1"
  },
  "peerDependencies": {
    "@vue-reactivity/watch": "^0.2.0",
    "@vue/reactivity": "^3.2.37",
    "@vue/shared": "^3.2.37",
    "gsap": "^3.10.4",
    "path-normalize": "^6.0.6"
  }
}