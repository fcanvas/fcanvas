{
  "name": "fcanvas",
  "version": "1.1.3",
  "description": "A powerful 2d canvas library that allows minimal rendering and provides diverse shapes.",
  "scripts": {
    "build": "pnpm auto-export && tsup",
    "auto-export": "tsx scripts/auto-export.ts",
    "dev": "vite dev",
    "prepublishOnly": "pnpm test && pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.browser.mjs",
  "jsdelivr": "dist/index.browser.global.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@thednp/dommatrix": "^2.0.2",
    "@types/plist": "^3.0.2",
    "canvas": "^2.11.0",
    "fetch-polyfill": "^0.8.2",
    "gsap": "^3.11.4",
    "happy-dom": "^8.4.4",
    "jsdom": "^20.0.3",
    "path2d-polyfill": "^2.0.1",
    "ts-file-exports": "^0.0.4",
    "tsup": "^6.6.3",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.2",
    "vitest": "^0.26.3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/fcanvas/fcanvas.git"
  },
  "homepage": "https://fcanvas.js.org",
  "keywords": [
    "fcanvas",
    "konva",
    "reactivity",
    "2d",
    "canvas",
    "Tachibana Shin",
    "tachibana-shin"
  ],
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format"
    }
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@vue/reactivity": "^3.2.47",
    "@vue/shared": "^3.2.47",
    "path-normalize": "^6.0.10",
    "tmx-tiledmap": "^1.2.1"
  },
  "peerDependencies": {
    "@vue/reactivity": "^3.2.47",
    "@vue/shared": "^3.2.37",
    "path-normalize": "^6.0.6"
  }
}
