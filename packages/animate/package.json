{
  "name": "@fcanvas/animate",
  "version": "1.0.22",
  "description": "Plugin that provides the ability to use animation for fcanvas",
  "scripts": {
    "build": "tsup",
    "dev": "vite dev",
    "prepublishOnly": "pnpm test && pnpm build",
    "test": "echo 'No test'",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.browser.mjs",
  "jsdelivr": "dist/index.global.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@vue/reactivity": "^3.2.45",
    "jsdom": "^20.0.3",
    "tsup": "^6.5.0",
    "tsx": "^3.12.1",
    "vite": "^4.0.3",
    "vitest": "^0.26.2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/tachibana-shin/fcanvas-next.git"
  },
  "homepage": "https://fcanvas.js.org",
  "keywords": [
    "fcanvas",
    "konva",
    "reactivity",
    "2d",
    "canvas",
    "animate",
    "animation",
    "gsap"
  ],
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format"
    }
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "fcanvas": "workspace:*",
    "gsap": "^3.11.4"
  },
  "peerDependencies": {
    "gsap": "^3.10.4"
  }
}
