{
  "name": "fcanvas",
  "version": "0.0.1",
  "description": "a monorepo for fcanvas",
  "scripts": {
    "build": "tsup",
    "dev": "tsx ./src/index.ts",
    "prepublishOnly": "pnpm test && pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.mjs",
  "jsdelivr": "dist/index.global.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "tsup": "^6.0.1",
    "vitest": "^0.13.0"
  },
  "dependencies": {
    "canvas": "^2.9.1",
    "fcanvas": "^0.1.4",
    "path2d-polyfill": "^1.2.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Tachibana-Shin/fcanvas-next.git"
  },
  "homepage": "https://fcanvas.js.org",
  "keywords": [
    "fcanvas-next",
    "Tachibana Shin",
    "Tachibana-Shin",
    "fcanvas-next",
    "fcanvas",
    "canvas",
    "2d",
    "html 5"
  ],
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format"
    }
  },
  "engines": {
    "node": ">=10"
  }
}
