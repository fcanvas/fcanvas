{
  "name": "@fcanvas/node",
  "version": "1.1.0-rc.31",
  "description": "Plugin allow use fcanvas in NodeJS",
  "scripts": {
    "build": "tsup",
    "dev": "vite dev",
    "prepublishOnly": "pnpm test && pnpm build",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "fcanvas": "workspace:*",
    "jsdom": "^20.0.3",
    "tsup": "^6.5.0",
    "tsx": "^3.12.1",
    "vitest": "^0.26.2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/tachibana-shin/fcanvas-next.git"
  },
  "homepage": "https://fcanvas.js.org",
  "keywords": [
    "fcanvas",
    "konva",
    "reactivity",
    "2d",
    "canvas",
    "tile",
    "tiles",
    "node",
    "fcanvas-node"
  ],
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format"
    }
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "canvas": "^2.11.0",
    "path2d-polyfill": "^2.0.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "fcanvas": "workspace:*"
  }
}
